<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 <hibernate-mapping>
 <class name="com.mr.user.User" table="user">
 <id name="id" column="id" type="int">
 <generator class="native"/>
 </id>
 <property name="username" type="string" length="45">
 <column name="username"/>
 </property>
 <property name="password" type="string" length="45">
 <column name="password"/>
 </property>
  <property name="sex" type="string" length="45">
 <column name="sex"/>
 </property>
 <property name="phone" type="string" length="45">
 <column name="phone"/>
 </property>
 <property name="email" type="string" length="45">
 <column name="email"/>
 </property>
  <property name="question" type="string" length="45">
 <column name="question"/>
 </property>
  <property name="answer" type="string" length="45">
 <column name="answer"/>
 </property>
  <property name="register" type="date" length="45">
 <column name="register"/>
 </property>
 </class>
 </hibernate-mapping>