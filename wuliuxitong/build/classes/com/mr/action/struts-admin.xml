<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC 
	"-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" 
	"http://struts.apache.org/dtds/struts-2.1.dtd" >
<struts>
	<!-- 后台管理 -->
	<!-- 登录 -->
	<package name="shop.admin" extends="struts-default">
		<action name="admin_denglu" method="denglu"  class="com.mr.action.AdminAction">
              <result name="input">/WEB-INF/Admin_hou/Denglu.jsp</result>
              <result name="success">/WEB-INF/Admin_hou/admin_shouye.jsp</result>
              <result name="admin_login">/WEB-INF/Admin_hou/Denglu.jsp</result>             
		</action>
		<!-- 会员管理 -->
		<action name="admin_user_*" method="{1}"  class="com.mr.action.UserAction">
              <result name="success">/WEB-INF/Admin_hou/show/User_message.jsp</result> 
              <result name="index">/WEB-INF/Admin_hou/Denglu.jsp</result>
              <result name="delete">/WEB-INF/Admin_hou/show/User_message.jsp</result>
              <result name="update_jsp">/WEB-INF/Admin_hou/update/User_update.jsp</result>
		</action>
	</package>
</struts>
