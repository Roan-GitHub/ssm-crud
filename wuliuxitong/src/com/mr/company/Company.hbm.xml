<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 <hibernate-mapping>
 <class name="com.mr.company.Company" table="company">
 <id name="id" column="id" type="int">
 <generator class="native"/>
 </id>
 <property name="name" type="string" length="45">
 <column name="name"/>
 </property>
 <property name="address" type="string" length="45">
 <column name="address"/>
 </property>
  <property name="phone" type="string" length="45">
 <column name="phone"/>
 </property>
 <property name="linkman" type="string" length="45">
 <column name="linkman"/>
 </property>
 <property name="info" type="string" length="45">
 <column name="info"/>
 </property>
 </class>
 </hibernate-mapping>